    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "messagebird": "^3.4.0",
    "nexus-prisma": "^0.15.0",
    "node-fetch": "^2.6.0",
    "signal-exit": "^3.0.3",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.4",
